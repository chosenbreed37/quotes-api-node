{
    "parser": "babel-eslint",
    "extends": [
        "airbnb/base",
        "plugin:import/errors"
    ],
    "rules": {
        "arrow-parens": 0,
        "comma-dangle": 0,
        "no-use-before-define": 0,
        "arrow-body-style": 0,
        "dot-notation": 0,
        "new-cap": 1,
        "no-console": 0,
        "no-unused-vars": 1,
        "indent": [
            2,
            4
        ],
        "prefer-template": 0
    },
    "env": {
        "mocha": true
    }
}
